if ($_.Name -like "*--*"){
    $scantype = 1
}
elseif ($_name -like "*--*" -or $_name -like "*--*" -or $_name -like "*--*" ) {
    $scantype = 2
}
else {
        Write-Host "The server type could not be defined"
        $errorcount++
        Continue
}
